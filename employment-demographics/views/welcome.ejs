<body class="nav-md">
<div class="container body">
  <div class="main_container">
    <div class="col-md-3 left_col">
      <div class="left_col scroll-view">
        <div class="navbar nav_title" style="border: 0;">
          <a href="index.html" class="site_title"><span>Demographics</span></a>
        </div>
        <div class="clearfix"></div>
        <br />
        <!-- menu profile quick info -->

        <!-- sidebar menu -->
        <div id="sidebar-menu" class="main_menu_side hidden-print main_menu">
          <div class="menu_section">
            <h3>General</h3>
            <ul class="nav side-menu">
              <li><a href="/"><i class="fa fa-home"></i> Home </a>
              </li>
              <li><a href="/employment"><i class="fa fa-briefcase"></i> Employment </a>
              </li>
              <li><a href="/education"><i class="fa fa-sitemap"></i> Education </a>
              </li>
              <li><a href="/salary"><i class="fa fa-bar-chart-o"></i> Jobs & Salary </a>
              </li>
            </ul>
          </div>
          <div class="menu_section">
          </div>
        </div>
        <!-- sidebar menu -->
      </div>
    </div>

    <!-- top navigation -->
    <div class="top_nav">
      <div class="nav_menu">
        <nav>
          <div class="nav toggle">
            <a id="menu_toggle"><i class="fa fa-bars"></i></a>
          </div>
          <ul class="nav navbar-nav navbar-right">
            <li class="">
              <a href="javascript:;" class="user-profile dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
                Sign In
              </a>
              <ul class="dropdown-menu dropdown-usermenu pull-right">
                <li><a href="/login">Sign In</a></li>
              </ul>
            </li>
          </ul>
        </nav>
      </div>
    </div>
    <!-- /top navigation -->

    <!-- page content -->
    <div class="right_col" role="main">
      <div class="row" style="visibility: ">
        <div class="col-md-12 col-sm-12 col-xs-12">
          <div class="dashboard_graph">
            <div class="row x_title">
              <div class="col-md-6">
                <h3>Age Distribution<small></small></h3>
              </div>
            </div>
            <div class="col-md-9 col-sm-9 col-xs-12">
              <div id="chart_plot_01" class="demo-placeholder"></div>
              <script>
                  // Age Distribution HighCharts
                  $(function () {
                      var chart;
                      $(document).ready(function() {
                          // $.getJSON("data.php", function(json) {

                          chart = new Highcharts.Chart({
                              chart: {
                                  renderTo: 'chart_plot_01',
                                  type: 'column',
                                  marginRight: 130,
                                  marginBottom: 25
                              },
                              title: {
                                  text: 'Number of disabled people in the age group',
                                  x: -20 //center
                              },
                              subtitle: {
                                  text: '',
                                  x: -20
                              },
                              xAxis: {
                                  categories: ['16-19', '19-24', '25-34', '35-44', '45-54', '55-64', '64 and above']
                              },
                              yAxis: {
                                  title: {
                                      text: 'Number of people'
                                  },
                                  plotLines: [{
                                      value: 0,
                                      width: 1,
                                      color: '#808080'
                                  }]
                              },
                              tooltip: {
                                  formatter: function() {
                                      return '<b>'+ this.series.name +'</b><br/>'+
                                          this.x +': '+ this.y;
                                  }
                              },
                              legend: {
                                  layout: 'vertical',
                                  align: 'right',
                                  verticalAlign: 'top',
                                  x: -10,
                                  y: 100,
                                  borderWidth: 0
                              },
                              series: [{
                                  name: 'Population',
                                  data: [<%= data[5].data[0].value%>, <%= data[8].data[0].value%>, <%= data[7].data[0].value%>,<%= data[11].data[0].value%>,<%= data[9].data[0].value%>,<%= data[10].data[0].value%>,<%= data[2].data[0].value%>]
                              }]
                          });
                          //  });
                      });
                  });
              </script>
            </div>
            <div class="clearfix"></div>
          </div>
        </div>
      </div>

      <br />

      <div class="">
        <div class="clearfix"></div>
        <div class="row">
          <div class="col-md-12 col-sm-12 col-xs-12">
          </div>
        </div>

        <div class="row">
          <div class="col-md-6 col-xs-12">

            <!--Population - Gender (M/F) - Highcharts-->
            <div class="x_panel">
              <div class="x_title">
                <h3>Gender Distribution<small></small></h3>
                <div class="clearfix"></div>
              </div>
              <div class="x_content">
                <div id="chart_plot_02" class="demo-placeholder"></div>
                <script>

                    // Gender Highcharts
                    $(function () {
                        var chart;
                        $(document).ready(function() {
                            // $.getJSON("data.php", function(json) {

                            chart = new Highcharts.Chart({
                                chart: {
                                    renderTo:'chart_plot_02',
                                    plotBackgroundColor: null,
                                    plotBorderWidth: null,
                                    plotShadow: false,
                                    type: 'pie'
                                },
                                title: {
                                    text: 'Gender Wise Disabled Working Population'
                                },
                                tooltip: {
                                    pointFormat: '{series.name}: <b>{point.percentage:.1f}%</b>'
                                },
                                plotOptions: {
                                    pie: {
                                        allowPointSelect: true,
                                        cursor: 'pointer',
                                        dataLabels: {
                                            enabled: true,
                                            format: '<b>{point.name}</b>: {point.percentage:.1f} %',
                                            style: {
                                                color: (Highcharts.theme && Highcharts.theme.contrastTextColor) || 'black'
                                            }
                                        }
                                    }
                                },
                                series: [{
                                    name: 'Gender',
                                    colorByPoint:true,
                                    data:[['Men',<%= data[0].data[0].value%>], ['Women',<%= data[1].data[0].value%>]]
                                }],
                            });
                            //  });
                        });
                    });
                </script>
              </div>
            </div>
          </div>


          <div class="col-md-6 col-xs-12">
            <!--Race/ Ethnicity Highcharts STARTS-->
            <div class="x_panel">
              <div class="x_title">
                <h3>Race/Ethnicity<small></small></h3>
                <div class="clearfix"></div>
              </div>
              <div class="x_content">
                <div id="chart_plot_03" class="demo-placeholder"></div>
                <script>
                    //Race/Ethnicity Highcharts
                    $(function () {
                        var chart;
                        $(document).ready(function() {
                            // $.getJSON("data.php", function(json) {

                            chart = new Highcharts.Chart({
                                chart: {
                                    renderTo:'chart_plot_03',
                                    plotBackgroundColor: null,
                                    plotBorderWidth: null,
                                    plotShadow: false,
                                    type: 'bar'
                                },
                                title: {
                                    text: 'Race/Ethnicity Wise Disabled Working Population'
                                },
                                xAxis: {
                                    categories: ['White', 'African American', 'Hispanic/Latino', 'Asian']
                                },
                                yAxis: {
                                    min: 0,
                                    title: {
                                        text: 'Percentage Population'
                                    }
                                },
                                legend: {
                                    reversed: true
                                },
                                plotOptions: {
                                    series: {
                                        stacking: 'normal'
                                    }
                                },
                                series: [{
                                    name: 'Race/Ethnicity',
                                    colorByPoint:true,
                                    data: [<%= data[4].data[0].value%>, <%= data[6].data[0].value%>, <%= data[12].data[0].value%>,<%= data[3].data[0].value%>]
                                }
                                ],
                            });
                            //  });
                        });
                    });
                </script>
              </div>
            </div>
            <!--Race/ Ethnicity Highcharts ENDS-->
          </div>
        </div>
      </div>
    </div>
    <!-- /page content -->
  </div>


  <!-- footer content -->
  <footer>
    <div class="pull-right">
      <a href="http://localhost:3000/">Disabled employees demographics</a>
    </div>
    <div class="clearfix"></div>
  </footer>
  <!-- /footer content -->
</div>
</div>

